.box6{overflow:hidden;
	  position:relative;
	  width:444px;
	  height:260px;
	  border:2px solid #ccc;
	  background:#fff;}
.box6 h3{float:left;
		 font-size:18px;
		 font-weight:bold;
		 letter-spacing:-1px;
		 background:#e1e1e1;
		 border-bottom:2px solid #ccc;}
.box6 h3.notice1{border-right:2px solid #ccc;}
.box6 h3 a{color:#444;
		   width:161px;
		   display:inline-block;
		   padding:25px 30px 20px 30px;}
.box6 ul{position:absolute;
		 top:90px;
		 left:20px;}
.box6 li{width:400px;
		 line-height:1.7;
		 font-size:14px;
		 letter-spacing:-1px;}
.box6 li a:hover{text-decoration:underline}
.box6 li .let{text-overflow:ellipsis;
		 white-space:nowrap;
		 width:310px;
		 overflow:hidden;
		 display:inline-block}
.no1{position:absolute;
	 top:17px;
	 left:180px;}
.no2{position:absolute;
	 top:17px;
	 right:20px;}
h3.on{background:#fff;
	  border:none;}
.date{float:right;}

.no{display:none;}

.box7{width:295px;
