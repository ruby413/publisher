@charset "utf-8";
@import url(reset.css);
@import url(header.css);
@import url(footer.css);

body{background:url(../sub_images/bg_color.gif) repeat 0 0;}
#wrapper{width:1400px;
		 margin:0 auto;
		 height:100%;
		 background:url(../main_images/main_bg.jpg) no-repeat -130px 0;
		 overflow:hidden;}
#wrapper.onn{background:url(../main_images/main_bg2.jpg) no-repeat -130px 0;}
#container{width:1040px;
		   float:right;
		   margin:170px 10px 0 0;
		   overflow:hidden;}
section{float:left;
		margin:0 0 20px 20px;}
section img{box-shadow:0px 0px 30px #d6d6d6;
			border:1px solid #d6d6d6;}
.box11{text-align:right;}
.box1{float:none;
	  margin-right:70px;
	  display:inline-block;}
.box4, .box7{margin-right:0px;}
.box1 img{border:none;}


.ico1{position:relative;}
.ico1 a:hover{position:absolute;
			animation:0.8s ease-in-out 0.6s alternate infinite exico}/*움직이는 시간/ 효과/ 정지시간/ 앞뒤이동 방향 / 무한반복/ 임의지정이름키프레임 */
	
	@keyframes exico{
		0%{
			top:0px;
		}
		100%{
			top:5px;
		}
	}