#skipMenu{position:absolute;
		  width:100%;
		  height:15px;}
#skipMenu a{position:absolute;
			top:-100px;}
#skipMenu a:hover,
#skipMenu a:active,
#skipMenu a:focus{position:absolute;
			      top:0;
				  width:100%;
				  padding:5px;
				  display:block;
				  text-align:center;
				  background:#999;
				  color:#fff;
				  font-size:16px;
				  font-weight:bold;}