.box7 li a{color:#ddd;}
.icon1{background:url(../images/btn7_icon1.gif) no-repeat 0 5px;}
.icon2{background:url(../images/btn7_icon2.gif) no-repeat 0 5px;}
.icon3{background:url(../images/btn7_icon3.gif) no-repeat 0 5px;}



