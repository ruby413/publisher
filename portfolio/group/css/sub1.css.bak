@charset:'utf-8';
@import url(reset.css);
@import url(header.css);
@import url(footer.css);
#container{width:1600px;
	   margin:20px auto;
	   background:#e6e7e8;
	   overflow:hidden;}
.gnb{margin:30px 0 0 20px;
	 float:left;}
.gnb li{width:346px;
		height:101px;
		overflow:hidden;
		position:relative;
		margin-bottom:10px;}
.gnb li span a:link{position:absolute;
					top:0;
					left:0}
.gnb li span a:hover{position:absolute;
					top:0;
					left:-346px;}


.btn{float:right;
	 margin:20px 10px 100px 30px;
	 overflow:hidden;
	 width:1150px;}
section{float:left;
		margin:10px 40px 10px 0;
		width:520px;
		height:250px;
		background:#fff;
		border-radius:5px;
		text-align:center;
		padding-top:15px;
		border:1px solid #c3c3c3;}
.box2,.box4,.box6,.box8,.box10,.box12,.box14,.box16,.box18{margin:10px 0;}