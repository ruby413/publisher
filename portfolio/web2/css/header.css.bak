@charset "utf-8";

#header_wrap{width:100%;
			 background:url(../main_img/headerbg.gif) repeat-x 0 0;
			 border-bottom:1px solid #c3c3c3;}
.header_top{width:1270px;
			height:42px;
			margin:0 auto;
			overflow:hidden;}
.topform{float:right;
		 width:170px;
		 height:27px;
		 background:#c3c3c3;
		 padding:13px 0 0 20px;
		 vertical-align:middle;}
.choice_box{background:#c3c3c3;}
.choice{font-size:14px;
	   font-weight:bold;
	   margin-right:5px;}
.topimg{float:right;
		margin-right:40px;}

nav{width:1600px;
	height:100px;
	margin:0 auto;
	overflow:hidden;}
h1{float:left;
   margin:10px 0 0 200px;}
.gnb > ul{margin-left:490px;}
.gnb > ul > li{float:left;
		   font-size:16px;
		   font-weight:bold;
	       padding:35px 40px 20px 40px;
		   text-align:center;
		   line-height:1.6;
		   padding-top:35px;
		   border:1px solid #fff;}
.gnb > ul > li > a{color:#525252;}
.gnb > ul > li > a:hover{color:#929292;}
.gnb span{display:block;
		  font-size:11px;
		  font-weight:normal;}

body{position:relative;}
#sub_gnb1{width:100%;
		  height:12px;
		  font-size:12px;
		  background:#444;
		  position:absolute;
		  top:141px;
		  left:0px;
		  z-index:9999;
		  display:none;}
.sub_gnb1{padding:12px 0 16px 280px;
		  font-weight:bold;}
.sub_gnb1 li a{color:#bcbcbc;}
.sub_gnb1 a:hover{text-decoration:underline}
.sub_gnb1 li{background:url(../sub_img/sub_bar.gif) no-repeat 20px 4px;
			 padding-left:45px;
			 float:left;}
.sub_gnb1 li:first-child{background:none;}

#gnb li.on ul{display:block;}

