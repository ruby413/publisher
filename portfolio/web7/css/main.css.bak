@charset "utf-8";
@import url(reset.css);
@import url(header.css);
@import url(footer.css);
@import url(pad.css);
@import url(phone.css);

#container{width:1600px;
		   margin:0 auto;
		   background:url(../images/mainbg.gif) repeat 0 0 /5px 130px;}
.visual_wrap{position:relative;
			 width:1600px;
			 height:572px;
		   overflow:hidden;}
.pic{overflow:hidden;
	 width:6400px;
	 position:absolute;
	 top:0;
	 left:0;}
.pic li{float:left;}

.dim{position:absolute;
	 top:30%;
	 left:30%;}
.pic_sec{color:#fff;
		 text-align:center;}
.pic_sec h2{font-weight:bold;
		    letter-spacing:-1px;
			font-size:28px;
			text-shadow:1px 1px 10px black;
			line-height:1.5;
			border-bottom:2px solid #fff;
			padding:25px 80px;}
.pic_sec ul{padding:40px;
			text-align:center;}
.pic_sec li{font-size:18px;
		    font-weight:bold;
			float:left;
			margin:0 20px;
			border:2px solid #fff;
			letter-spacing:-1px;}
.pic_sec li a{color:#fff;
			  width:120px;
			  height:30px;
			  padding-top:15px;
			  display:inline-block}
.pic_sec li a:hover{background-color:rgba(0,0,0,0.8);}
.color{color:#a3e8ff;}

.btn{position:absolute;
	 bottom:20px;
	 right:7%;
	 opacity:0.7}


.contents{width:1300px;
		  margin:0 auto;
		  padding:80px 0;
		  overflow:hidden;}
.contents section{float:left;
				  margin:0 20px 20px 0;}

.box1{position:relative;
	  width:326px;
	  border:1px solid #ccc;}
.box1 h3{position:absolute;
		 bottom:30px;
		 right:30px;
		 font-size:16px;
		 text-shadow:1px 1px 10px black;
		 font-weight:bold;
		 color:#fff;
		 z-index:11;}
.bg1{height:325px;
	width:326px;
	position:absolute;
	top:0;
	left:0;
	background-color:rgba(0,0,0,0);}
.bg1:hover{background-color:rgba(0,0,0,0.2);}

